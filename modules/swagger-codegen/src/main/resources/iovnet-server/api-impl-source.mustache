void {{classname}}Impl::create_{{classVarName}}_by_id(const std::string &name, const {{baseName}}Schema &schema) {
  if (iomodules.count(name) != 0) {
    throw std::runtime_error("There is already an IOModule with name " + name);
  }

  {{baseName}}Schema localSchema(schema);
  auto iter;
  bool inserted;

  if(localSchema.type_IsSet() && !localSchema.get_Type().empty()) {
    switch(localSchema.get_Type()) {
      case "TYPE_XDP_SKB":
        std::tie(iter, inserted) = iomodules.emplace(std::piecewise_construct,
                                                     std::forward_as_tuple(name),
                                                     std::forward_as_tuple(name, schema, IOModuleType::TYPE_XDP_SKB));
        break;
      case "TYPE_XDP_SKB":
        std::tie(iter, inserted) = iomodules.emplace(std::piecewise_construct,
                                                     std::forward_as_tuple(name),
                                                     std::forward_as_tuple(name, schema, IOModuleType::TYPE_XDP_DRV));
        break;
      default:
        std::tie(iter, inserted) = iomodules.emplace(std::piecewise_construct,
                                                     std::forward_as_tuple(name),
                                                     std::forward_as_tuple(name, schema));
    }
  } else {
    std::tie(iter, inserted) = iomodules.emplace(std::piecewise_construct,
                                                 std::forward_as_tuple(name),
                                                 std::forward_as_tuple(name, schema));
  }
}

